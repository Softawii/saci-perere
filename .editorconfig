# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = true

[{**.js,**.vue,.eslintrc,package.json,**.yaml}]
trim_trailing_whitespace = true
indent_size = 2
